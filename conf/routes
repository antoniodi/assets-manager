# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
POST    /command/:command            co.com.application.controllers.commands.CommandController.executeCommand( command: String )
GET     /user-zio/:username          co.com.application.controllers.queries.FindUserQueryZIO.findUser( username: String )
GET     /users                       co.com.application.controllers.queries.FindUsersQuery.find()
#GET    /posts                       co.com.application.controllers.queries.RequestPosts.request
GET     /version                     co.com.application.controllers.queries.VersionQuery.version
